{
  "name": "bruno-parienti-portfolio",
  "version": "1.0.0",
  "description": "Portfolio professionnel one-page pour Bruno Parienti - Acteur",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run css:watch\" \"nodemon server.js\"",
    "start": "node server.js",
    "css:build": "tailwindcss -i ./public/css/input.css -o ./public/css/styles.css --minify",
    "css:watch": "tailwindcss -i ./public/css/input.css -o ./public/css/styles.css --watch",
    "build": "npm run css:build",
    "vercel-build": "npm run css:build"
  },
  "keywords": [
    "portfolio",
    "actor",
    "bruno-parienti",
    "express",
    "ejs",
    "tailwindcss"
  ],
  "author": "Bruno Parienti",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "helmet": "^7.1.0",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
